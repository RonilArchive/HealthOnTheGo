@CHARSET "US-ASCII";


  .Feedback
  {
  position:fixed;
  left:102%;
  top:60%;
  width:165px;
  height:280px;
  transform:rotate(270deg);
  -ms-transform:rotate(270deg); /* IE 9 */
  -webkit-transform:rotate(270deg); /* Safari and Chrome */
  background-color:rgba(11, 139, 0, 0.55);
  font-family: HOTGFont;
  font-size:17px;
  color:white;
  text-align:center;
  border:2px solid Green;
  border-radius:4px;
  border-style:outset;
  -webkit-transition: left 1s; 
  z-index:100;
  
  }
  
  .Feedback:hover
  {
  
  left:86%;
  
  }
  