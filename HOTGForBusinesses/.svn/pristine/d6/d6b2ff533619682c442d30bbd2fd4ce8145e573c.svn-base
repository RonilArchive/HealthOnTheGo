<%@page contentType="text/html" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <title>Chemist Login</title>
        <script language="javascript" type="text/javascript" src="niceforms.js"></script>
        <link rel="stylesheet" type="text/css" media="all" href="niceforms-default.css" />
    </head>
    <body>
        <div id="container">
        <form action="/cValidate" method="post" class="niceform">
        <fieldset>
        <legend>Chemist Login</legend>  
        <dl><dt>
            <label for="clogin">Login:</label>
            </dt><dd><input type="text" name="clogin" id="clogin" size="40" maxlength="128" />
            </dd></dl>
         <dl>
             <dt><label for="cpassword">Password:</label></dt>
            <dd><input type="password" name="cpassword" id="cpassword" size="40" maxlength="128" />
            </dd></dl>  
        </fieldset>
        <fieldset><center><input type="submit" name="submit" id="submit" value="Login" /></center></fieldset>
    </body>
</html>